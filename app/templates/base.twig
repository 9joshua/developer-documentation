<!DOCTYPE html>
<html>
<head>
    <title>{% if title|default is not empty %}{{ title|striptags|raw }} - {% endif %}Piwik Analytics - Developer Docs</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/css/bootstrap.min.css?{{ revision|e('html_attr') }}" rel="stylesheet">
    <link href="/css/font-awesome.min.css?{{ revision|e('html_attr') }}" rel="stylesheet">
    <link href="/css/highlightjs/github.css?{{ revision|e('html_attr') }}" rel="stylesheet" media="screen">
    <link href="/css/app.css?{{ revision|e('html_attr') }}" rel="stylesheet" media="screen">
    <link href="/css/sidebar.css?{{ revision|e('html_attr') }}" rel="stylesheet" media="screen">
    <link href="/css/documentation.css?{{ revision|e('html_attr') }}" rel="stylesheet" media="screen">
    <link href="//fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,400,300,600&amp;subset=latin,cyrillic-ext,greek-ext,greek,vietnamese,latin-ext,cyrillic" rel="stylesheet" type="text/css">
    <!--[if lt IE 9]>
    <script src="/js/html5shiv.3.7.0.js?{{ revision|e('html_attr') }}"></script>
    <script src="/js/respond.min.1.3.0.js?{{ revision|e('html_attr') }}"></script>
    <![endif]-->
</head>
<body>

<div id="menu" class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand hidden-sm" href="/"><img height="20" width="74" src="/img/piwik-logo.png"/></a>
        </div>
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li {% if isHome %}class="active"{% endif %}><a href="/">Home</a></li>
                <li {% if activeMenu == 'Integrate' %}class="active"{% endif %}>
                    <a href="/integration">Integrate</a>
                </li>
                <li {% if activeMenu == 'Develop' %}class="active"{% endif %}>
                    <a href="/develop">Develop</a>
                </li>
                <li {% if activeMenu == 'API Reference' %}class="active"{% endif %}>
                    <a href="/api-reference">API Reference</a>
                </li>
                <li {% if activeMenu == 'Changelog' %}class="active"{% endif %}>
                    <a href="/changelog">Changelog</a>
                </li>
                <li {% if activeMenu == 'Support' %}class="active"{% endif %}>
                    <a href="/support">Support</a>
                </li>
            </ul>
            <ul class="nav navbar-nav pull-right">
                <li>
                    <div id="quick-search-typeahead">
                        <input type="text" placeholder="Quick search..."/>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</div>

{% block headline %}
    {% if title is defined and title and (setPageHeader is not defined or setPageHeader) %}
        <div class="container">
            <div class="page-header"><h1>{{ title }}</h1></div>
        </div>
    {% endif %}
{% endblock %}

{% if noContainer is not defined %}
<div class="container">
{% endif %}
    {% block content %}
        {{ content|raw }}
    {% endblock %}
{% if noContainer is not defined %}
</div>
{% endif %}

{% block footer %}
    <footer>
        <div class="container">
            <p class="pull-left">
                &copy; <a href="http://piwik.org">Piwik.org</a>
                |
                <a href="http://piwik.org/blog/2014/05/piwik-expanding-seeking-talented-software-engineer-new-zealand-poland/" title="We're hiring a software engineer!">Jobs</a>
                |
                <a href="https://twitter.com/piwik"><i class="fa fa-twitter"></i> Twitter</a>
                |
                <a href="https://github.com/piwik"><i class="fa fa-github"></i> GitHub</a>
            </p>
            <p class="pull-right">{% if linkToEditDocument is defined and linkToEditDocument %}
            <a href="{{ linkToEditDocument }}" target="_blank">Edit on GitHub</a>
                {% endif %}</p>
            <p class="pull-right">
                <a href="mailto:developer@piwik.org?subject=Feedback%20Developer%20Guides">Give Feedback</a>{% if linkToEditDocument is defined and linkToEditDocument %}&nbsp;|&nbsp;{% endif %}
            </p>
        </div>
    </footer>
{% endblock %}

<script src="/js/jquery.js?{{ revision|e('html_attr') }}"></script>
<script src="/js/bootstrap.min.js?{{ revision|e('html_attr') }}"></script>
<script src="/js/bootstrap2-typeahead.js?{{ revision|e('html_attr') }}"></script>
<script src="/js/app.js?{{ revision|e('html_attr') }}"></script>
<script src="/js/highlight.pack.js?{{ revision|e('html_attr') }}"></script>
<script type="text/javascript">
$(document).ready(function () {
    hljs.initHighlightingOnLoad();
});
</script>

<!-- Piwik -->
<script type="text/javascript">

    var _paq = _paq || [];
    _paq.push(["setCookieDomain", "*.piwik.org"]);
    {% block piwik_customization %}{% endblock %}
    _paq.push(["trackPageView"]);
    _paq.push(["enableLinkTracking"]);

    function track() {

        (function() {
            var u=(("https:" == document.location.protocol) ? "https" : "http") + "://demo.piwik.org/";
            _paq.push(["setTrackerUrl", u+"piwik.php"]);
            _paq.push(["setSiteId", "36"]);
            var d=document, g=d.createElement("script"), s=d.getElementsByTagName("script")[0]; g.type="text/javascript";
            g.defer=true; g.async=true; g.src=u+"piwik.js"; s.parentNode.insertBefore(g,s);
        })();
    }

    if (window.addEventListener) { window.addEventListener("load", track, false); }
    else if (window.attachEvent) { window.attachEvent("onload",track); }
    else {
        track();
    }
</script>
<!-- End Piwik Code -->

</body>
</html>
